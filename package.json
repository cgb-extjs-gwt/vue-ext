{
    "name": "@softvisio/vue-ext",
    "version": "8.0.2",
    "description": "Softisio Vue Ext",
    "keywords": [
        "softvisio",
        "vue",
        "extjs"
    ],
    "homepage": "https://softvisio.github.io/vue-ext/",
    "bugs": {
        "url": "https://github.com/softvisio/vue-ext/issues",
        "email": "zdm <zdm@softvisio.net>"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/softvisio/vue-ext.git"
    },
    "license": "ISC",
    "author": "zdm <zdm@softvisio.net>",
    "type": "module",
    "imports": {
        "#core/*": "@softvisio/core/*",
        "#resources/*": "./resources/*",
        "#lib/*": [
            "./lib/*.vue",
            "./lib/*.js",
            "./lib/*.cjs"
        ]
    },
    "exports": {
        ".": "./lib/index.js",
        "./*": [
            "./lib/*.js",
            "./lib/*.vue",
            "./lib/*/index.js",
            "./lib/*/index.vue"
        ],
        "./resources/*": "./resources/*"
    },
    "dependencies": {
        "@amcharts/amcharts5": "^5.1.10",
        "@fortawesome/fontawesome-free": "^6.0.0",
        "@softvisio/ext": "^5.3.2",
        "froala-editor": "^4.0.0",
        "pdfjs-dist": "^3.0.279"
    },
    "peerDependencies": {
        "@softvisio/vue": "^7.0.0"
    }
}
