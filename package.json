{
    "name": "@softvisio/vue-ext",
    "version": "8.10.40",
    "description": "Softisio Vue Ext",
    "keywords": [
        "softvisio",
        "vue",
        "extjs"
    ],
    "homepage": "https://softvisio-node.github.io/vue-ext/",
    "bugs": {
        "url": "https://github.com/softvisio-node/vue-ext/issues",
        "email": "zdm <zdm@softvisio.net>"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/softvisio-node/vue-ext.git"
    },
    "license": "ISC",
    "author": "zdm <zdm@softvisio.net>",
    "type": "module",
    "imports": {
        "#core/*": "@softvisio/core/*",
        "#resources/*": "./resources/*",
        "#src/*": [
            "./src/*.vue",
            "./src/*.js",
            "./src/*/index.vue",
            "./src/*/index.js"
        ]
    },
    "exports": {
        "./*": [
            "./src/*.vue",
            "./src/*.js",
            "./src/*/index.vue",
            "./src/*/index.js"
        ],
        "./resources/*": "./resources/*",
        "./webpack-components/*": "./resources/webpack-components/*/component.js"
    },
    "files": [
        "/bin",
        "/lib",
        "/resources",
        "/src"
    ],
    "dependencies": {
        "@amcharts/amcharts5": "^5.1.10",
        "@fortawesome/fontawesome-free": "^6.0.0",
        "@softvisio/ext": "^5.3.2",
        "froala-editor": "^4.0.0",
        "pdfjs-dist": "^4.0.0"
    },
    "peerDependencies": {
        "@softvisio/vue": "^7.0.0"
    }
}
