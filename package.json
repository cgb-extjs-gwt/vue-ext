{
    "name": "@softvisio/vue-ext",
    "version": "5.0.0",
    "description": "Softisio Vue Ext",
    "keywords": [
        "softvisio",
        "vue",
        "extjs"
    ],
    "homepage": "https://github.com/softvisio/vue-ext",
    "bugs": {
        "url": "https://github.com/softvisio/vue-ext/issues",
        "email": "zdm@softvisio.net"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/softvisio/vue-ext.git"
    },
    "author": "zdm <zdm@softvisio.net>",
    "type": "module",
    "imports": {
        "#core/*": "@softvisio/core/*",
        "#lib/*": "./lib/*.js"
    },
    "exports": {
        ".": "./lib/index.js",
        "./*": [
            "./lib/*.js",
            "./lib/*.vue",
            "./lib/*/index.js",
            "./lib/*/index.vue"
        ],
        "./resources/*": "./resources/*",
        "./vue.config.cjs": "./resources/vue.config.cjs",
        "./babel.config.cjs": "./resources/babel.config.cjs",
        "./webpack.config.cjs": "./resources/webpack.config.cjs"
    },
    "dependencies": {
        "@amcharts/amcharts4": "^4.10.18",
        "@fortawesome/fontawesome-free": "^5.15.3",
        "@softvisio/ext": "github:softvisio/ext#semver:^5.0.0",
        "froala-editor": "^4.0.0",
        "pdfjs-dist": "^2.9.359"
    },
    "peerDependencies": {
        "@softvisio/vue": "github:softvisio/vue#main"
    }
}
