{
    "name": "@softvisio/vue-ext",
    "version": "3.0.0-beta.0",
    "description": "Softisio Vue Ext",
    "keywords": [
        "softvisio",
        "vue",
        "extjs"
    ],
    "homepage": "https://bitbucket.org/softvisio/softvisio-vue-ext",
    "bugs": {
        "url": "https://bitbucket.org/softvisio/softvisio-vue-ext/issues",
        "email": "zdm@softvisio.net"
    },
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/softvisio/softvisio-vue-ext.git"
    },
    "author": "zdm <zdm@softvisio.net>",
    "type": "module",
    "imports": {
        "#core/*": "@softvisio/core/*"
    },
    "exports": {
        ".": "./lib/index.js",
        "./*": [
            "./lib/*.js",
            "./lib/*.vue",
            "./lib/*/index.js",
            "./lib/*/index.vue"
        ],
        "./resources/*": "./resources/*"
    },
    "scripts": {
        "lint": "softvisio-cli lint"
    },
    "peerDependencies": {
        "@amcharts/amcharts4": "^4.10.13",
        "@fortawesome/fontawesome-free": "^5.15.2",
        "@softvisio/ext": "bitbucket:softvisio/softvisio-ext#semver:^3.0.0",
        "@softvisio/vue": "^3.0.0-alpha.4",
        "froala-editor": "^3.2.6",
        "pdfjs-dist": "^2.6.347"
    },
    "lint": [
        "*.*",
        "lib/**",
        "resources/**"
    ]
}
